{
  "name": "Get That API",
  "description": "Get That API is an browser extension made to test API endpoints right in the browser",
  "version": "2.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
